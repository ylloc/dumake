$source = main.cpp
$output = build.out
$compiler = clang++
$flags = -std=c++23
$dir = test

<build>
commands = [
    'mkdir $dir',
    'cd $dir',
    '$compiler $flags -o ../$source'
]

<log>
command = ''

<test>: [<log>]
command = '_'

<run>: [<build>, <log>]
command = './test/a.out'

<execute>
run